    "autoprefixer": "10.4.16",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-svelte": "2.34.0",
    "husky": "8.0.3",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.5",
    "svelte": "4.2.1",
    "tailwindcss": "3.3.3",
    "vite": "4.4.11"
  }
